  "bugs": {
    "url": "https://github.com/mymoid/mymoid-js/issues"
  },
  "homepage": "https://github.com/mymoid/mymoid-js#readme",
  "dependencies": {
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "tsup": "^8.0.1",
    "typedoc": "^0.25.4",
    "vitest": "^1.0.4"
  }
}
