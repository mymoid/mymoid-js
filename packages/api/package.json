{
  "name": "mymoid-api",
  "version": "0.0.0",
  "description": "The client for interacting with the MYMOID API.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "docs": "typedoc --options ./typedoc.js src",
    "build": "tsup src/index.ts --format esm,cjs --dts && npm run docs",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "test": "vitest run --typecheck"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mymoid/mymoid-js.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mymoid/mymoid-js/issues"
  },
  "homepage": "https://github.com/mymoid/mymoid-js#readme",
  "dependencies": {
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.7",
    "tsup": "^8.0.1",
    "typedoc": "^0.25.4",
    "vitest": "^1.1.3"
  }
}
